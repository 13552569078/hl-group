{
  "name": "fx-front-validators",
  "version": "1.0.0",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/fx-front-validators.umd.js",
  "module": "./dist/fx-front-validators.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fx-front-validators.es.js",
      "require": "./dist/fx-front-validators.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "pub": "npm publish",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:publish": "sh ./scripts/publish-docs.sh"
  },
  "dependencies": {
    "async-validator": "^4.0.7",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/qs": "^6.9.7",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-define-config": "^1.2.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "jsdom": "^19.0.0",
    "hongluan-ui": "^2.2.3",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.5.4",
    "unplugin-vue-define-options": "^0.6.0",
    "vite": "^2.9.9",
    "vitepress": "^0.22.4",
    "vitest": "^0.15.1",
    "vue": "^3.2.0",
    "vue-tsc": "^0.37.0"
  }
}